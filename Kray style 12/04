/* Адаптация для смартфонов (макс ширина: 768px) */
@media only screen and (max-width: 768px) {
  /* Общие настройки */
  html, body {
    font-size: 14px !important;
    line-height: 1.4 !important;
    margin: 0;
    padding: 0;
    box-sizing: border-box;
  }
  
  /* Переопределение фоновой картинки: позиционируем слева,
     чтобы справа оставалось пустое пространство, и изменяем размер */
  body {
    background-position: left center !important;
    background-size: auto 100% !important;
  }
  
  /* Адаптация изображений и медиа – чтобы не выходили за пределы экрана */
  img, video {
    max-width: 100% !important;
    height: auto !important;
  }
  
  /* Универсальное центрирование: примените этот класс к тем элементам, которые нужно центрировать */
  .center-mobile {
    margin-left: auto !important;
    margin-right: auto !important;
    display: block;
  }
  
  /* Отмена float для всех элементов с классами, содержащими "float-" */
  [class*="float-"] {
    float: none !important;
    width: auto !important;
  }
  
  /* Автоматическое расширение контейнеров для адаптации ширины */
  .container, .wrapper, .content, .profile, .header, .footer, .nav {
    width: 100% !important;
    padding-left: 10px !important;
    padding-right: 10px !important;
    box-sizing: border-box;
  }
  
  /* Корректировка размеров заголовков для удобного чтения на маленьких экранах */
  h1 {
    font-size: 22px !important;
  }
  h2 {
    font-size: 20px !important;
  }
  h3 {
    font-size: 18px !important;
  }
  h4, h5, h6 {
    font-size: 16px !important;
  }
  
  /* Увеличение области клика для навигационных ссылок */
  nav a, .nav a {
    padding: 10px 15px !important;
    font-size: 16px !important;
  }
  
  /* Если необходимо скрывать отдельные элементы на мобильном, назначьте им класс .hide-on-mobile */
  .hide-on-mobile {
    display: none !important;
  }
}
@media only screen and (max-width: 768px) {
  body {
    /* Отменяем background-attachment fixed, чтобы избежать проблем на мобильных */
    background-attachment: scroll !important;
    /* Смещаем фон влево, чтобы справа оставался пустой участок */
    background-position: -50px center !important;
    /* Фон повторяется не будет */
    background-repeat: no-repeat !important;
    /* Масштабируем фон: высота 80% (можно варьировать для достижения нужного эффекта) */
    background-size: auto 80% !important;
  }
